public class quizq2{
  public static void main(String args[]){
    java.util.Scanner abesh=new java.util.Scanner (System.in);
    System.out.println("Enter quantity");
   
    int q=abesh.nextInt();
    if(q<4){
      System.out.println("error ");
      break;
    }
    int []array=new int[q];
    for(int i=0;i<array.length;i++){
      array[i]=abesh.nextInt();
    }
    for(int i=0;i<array.length;i++){
      for(int j=i+1;j<array.length;i++){
    if(array [i]> array[j]){
      int temp=array[i];
      array[i]=array[j];
      array[j]=array[i];
    }
  }
    }
    if(q%2!=0){
      int sum=(array[q/2]+array[q/2+1]+array[q/2-1];
               int avg=sum/3;
               }
else {
  int sum=(array[q/2]+array[q/2+1]+array[q/2-1]+array[q/2-2]);
  int avg=sum/4;
}
}
}